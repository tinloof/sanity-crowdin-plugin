{
  "name": "sanity-crowdin-plugin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p next:dev sanity:typegen:watch",
    "next:dev": "next dev",
    "build": "run-s sanity:typegen next:build",
    "next:build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write --ignore-unknown .",
    "typecheck": "tsc --noEmit",
    "sanity:typegen:watch": "watch \"npm run sanity:typegen\" ./sanity ./data",
    "sanity:typegen": "sanity schema extract --path ./types/schema.json && sanity typegen generate"
  },
  "dependencies": {
    "@crowdin/crowdin-api-client": "^1.46.0",
    "@next/env": "^14.2.4",
    "@portabletext/react": "^3.1.0",
    "@sanity/asset-utils": "^1.3.0",
    "@sanity/client": "^6.20.1",
    "@sanity/document-internationalization": "^3.3.3",
    "@sanity/icons": "^3.2.0",
    "@sanity/image-url": "^1.0.2",
    "@sanity/orderable-document-list": "^1.2.1",
    "@sanity/preview-url-secret": "^1.6.17",
    "@sanity/react-loader": "^1.10.3",
    "@sanity/ui": "^2.8.24",
    "@sanity/util": "^4.0.1",
    "@sanity/uuid": "^3.0.2",
    "@sanity/vision": "^3.48.1",
    "@tinloof/sanity-studio": "^1.7.0",
    "@tinloof/sanity-web": "^0.6.0",
    "cheerio": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "cva": "npm:class-variance-authority@^0.7.0",
    "jssha": "^3.3.1",
    "lodash": "^4.17.21",
    "next": "15.0.3",
    "next-sanity": "^9.8.16",
    "react": "19.0.0-rc-7c8e5e7a-20241101",
    "react-dom": "19.0.0-rc-7c8e5e7a-20241101",
    "react-icons": "^5.3.0",
    "sanity": "^3.64.1",
    "sanity-plugin-media": "^2.3.2",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@portabletext/types": "^2.0.13",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-perfectionist": "^2.11.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.39",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0",
    "watch": "^1.0.2"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    }
  }
}
